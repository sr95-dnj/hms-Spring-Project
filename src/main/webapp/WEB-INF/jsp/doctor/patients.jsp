<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<jsp:include page="/WEB-INF/jsp/doctor/hf/header.jsp"/>

<div class="page-wrapper">
    <div class="content">
        <div class="row">
            <div class="col-sm-4 col-3">
                <h4 class="page-title">Patients</h4>
            </div>
            <div class="col-sm-8 col-9 text-right m-b-20">
                <a href="/prescription/preList" class="btn btn btn-primary btn-rounded float-right"><i class="fa fa-plus"></i> View All Prescription</a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table class="table table-border table-striped custom-table datatable mb-0">
                        <thead>

                            <tr>
                                <th>Id</th>
                                
                                <th>Update</th>
                                
                                <th>Name</th>
                                <th>Department</th>
                                <th>Reference Doctor</th>
                                <th>Age</th>
                                <th>Blood Group</th>
                                <th>Disease</th>
                                <th>Room No</th>
                                <th>Email</th>
                                <th>Mobile</th>
                                <th>Address</th>
                                <th>Admitted Date</th>
                                <th>Comment</th>
                                <th class="text-right">Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <c:forEach items="${map.pList}" var="patient">
                                <tr>
                                    <td>${patient.id}</td>
                                   
                                    <td><span class="custom-badge status-green">
                                        <a class="dropdown-item" href="/doctor/pescriptionView/${patient.id}">Take Up</a>
                                        
                                    </span></td>
                                    
                                    <td>${patient.name}</td>
                                    <td>${patient.department}</td>
                                    <td>${patient.referenceDoctor}</td>
                                    <td>${patient.age}</td>
                                    <td>${patient.bloodGroup}</td>
                                    <td>${patient.disease}</td>
                                    <td>${patient.roomNo}</td>
                                    <td>${patient.email}</td>
                                    <td>${patient.mobile}</td>
                                    <td>${patient.address}</td>
                                    <td>${patient.admitedDate}</td>
                                    <td>${patient.comment}</td>
                                    <td class="text-right">
                                        <div class="dropdown dropdown-action">
                                            <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown" aria-expanded="false"><i class="fa fa-ellipsis-v"></i></a>
                                            <div class="dropdown-menu dropdown-menu-right">
                                                <a class="dropdown-item" href="/patient/edit/${patient.id}"><i class="fa fa-pencil m-r-5"></i> Edit</a>
                                                <a class="dropdown-item" href="/patient/delete/${patient.id}"><i class="fa fa-trash-o m-r-5"></i> Delete</a>
                                            </div>
                                        </div>
                                    </td>

                                </tr>
                            </c:forEach>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
