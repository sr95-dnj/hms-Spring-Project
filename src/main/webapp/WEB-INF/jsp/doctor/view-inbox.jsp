<jsp:include page="/WEB-INF/jsp/doctor/hf/header.jsp"/>

<div class="page-wrapper">
    <div class="content">
        <div class="row">
            <div class="col-sm-12">
                <h4 class="page-title">View Message</h4>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12">
                <div class="card-box">
                    <div class="mailview-content">
                        <div class="mailview-header">
                            <div class="row">
                                <div class="col-sm-9">
                                    <div class="text-ellipsis m-b-10">
                                        <span class="mail-view-title">${inbox.subject}</span>
                                    </div>
                                </div>
                                <div class="col-sm-3">
                                    <div class="mail-view-action">
                                        <div class="btn-group">
                                            <button type="button" class="btn btn-white btn-sm" data-toggle="tooltip" title="Delete"> <i class="fa fa-trash-o"></i></button>
                                            
                                        </div>
                                        <button type="button" class="btn btn-white btn-sm" data-toggle="tooltip" title="Print"> <i class="fa fa-print"></i></button>
                                    </div>
                                </div>
                            </div>
                            <div class="sender-info">
                                <div class="sender-img">
                                    <img width="40" alt="" src="assets/img/user.jpg" class="rounded-circle">
                                </div>
                                <div class="receiver-details float-left">
                                    <span class="sender-name">${inbox.email}</span>
                                    <span class="receiver-name">
                                        to <span>me</span>, <span>${inbox.toDoctor}</span>, <span>Paul</span>
                                    </span>
                                </div>
                                <div class="mail-sent-time">
                                    <span class="mail-time">${inbox.dateTime}</span>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </div>
                        <div class="mailview-inner">
                            
                           <br/><br/>
                            <p>Hello,</p>
                            <p>${inbox.message}</p>
                            <p>Thanks,
                                <br></p>
                        </div>
                    </div>
                    
                    <div class="mailview-footer">
                        <div class="row">
                            <div class="col-sm-6 left-action">
                                <a href="/doctorInbox/reply/${inbox.id}"><button type="button" class="btn btn-white"><i class="fa fa-reply"></i> Reply</button></a>
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
