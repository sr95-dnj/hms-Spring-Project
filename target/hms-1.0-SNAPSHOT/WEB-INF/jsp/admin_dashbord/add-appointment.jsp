
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<jsp:include page="/WEB-INF/jsp/common/dashbord/header.jsp"/>


    <div class="page-wrapper">
        <div class="content">
            <div class="row">
                <div class="col-lg-8 offset-lg-2">
                    <h4 class="page-title">Add Appointment</h4>
                </div>
                <div class="col-md-4">
                <div class="search-box">
                    <form action="/approve/getDetails" method="post">
                        <div class="input-group input-group-sm">
                           <select class="select" name="first_name">
                                    <option>Select Doctor</option>
                                    <c:forEach items="${map.docList}" var="doc">
                                        <option value="${doc.first_name}">${doc.first_name}</option>
                                    </c:forEach>
                                </select>
                            <span class="input-group-append">
                                <button class="btn" type="submit"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                    </form>
                </div>
            </div>
            </div>
            <div class="row">
                <div class="col-lg-8 offset-lg-2">
                    <form action="/approve/save" method="post">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Appointment Date</label>
                                    <input class="form-control" type="datetime-local" value="APT-0001" name="appointDate">
                                </div>
                               
                                    <input class="form-control" type="hidden" value="${map.doctor.code}" name="doctorCode">
                                
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Patient Name</label>
                                    <input class="form-control" type="text" name="name">
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Patient Address</label>
                                    <input class="form-control" type="text" name="address">
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Patient Mobile</label>
                                    <input class="form-control" type="text" name="mobile">
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Doctor</label>
                                    <input class="form-control" type="text" name="doctor" value="${map.doctor.first_name}">
                                </div>
                                
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label>Department</label>
                                    
                                    <input class="form-control" type="text" name="doctor" value="${map.doctor.department}">
                                         
                                </div>
                            </div>
                        </div>
                       
                       
                        <div class="form-group">
                            <label>Message</label>
                            <textarea cols="30" rows="4" class="form-control" name="comment"></textarea>
                        </div>
                       
                        <div class="m-t-20 text-center">
                            <button class="btn btn-primary submit-btn">Create Appointment</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
